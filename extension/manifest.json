{
  "name": "CodesignalMock",
  "description": "",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": "icons/chiikawaIcon.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["shared/leetcode-problems.csv"],
      "matches": ["<all_urls>"]
    }
  ],
  "host_permissions": [
    "http://localhost:3000/*",
    "http://localhost:5000/"
  ],
  "permissions": ["tabs", "activeTab", "storage", "scripting", "identity", "alarms"]
}
